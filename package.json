    "chart.js": "^4.4.0",
    "dotenv": "^17.2.2",
    "firebase": "^10.4.0",
    "jspdf": "^3.0.2",
    "jspdf-autotable": "^5.0.2",
    "jwt-decode": "^3.1.2",
